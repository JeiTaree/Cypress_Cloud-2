<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Cypress_Cloud.PTIBooking">


	
  <StackLayout Padding="30">
    


    <Label x:Name="Name" FontSize="10" Text="Enter Parent's Full Name"/> <Entry></Entry>
	<Label x:Name="UserID" FontSize="10" Text="Enter ID"/> <Entry></Entry>
	<Label x:Name="Email" FontSize="10" Text="E-Mail"/><Entry></Entry>


	<Label FontSize="10" Text="Select Date and Time"/>
	<DatePicker></DatePicker>
	<TimePicker></TimePicker>
	<Button Clicked="SaveData_Buton_Clicked" FontSize="10" Text="Done"/>

  </StackLayout> 


</ContentPage>